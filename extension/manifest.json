{
  "name": "Final Project",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "<all_urls>",
    "http://*/",
    "https://*/",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": ["background.js", "jquery.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "/extension/favicon.jpg"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"]
    },
    {
      "js": ["jquery.js"],
      "matches": ["<all_urls>"]
    },
    {
      "js": ["dictionaryCreation.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
